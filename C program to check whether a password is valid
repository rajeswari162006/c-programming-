#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char password[100];
    int i, hasDigit = 0;

    printf("Enter password: ");
    scanf("%s", password);

    if(strlen(password) < 6) {
        printf("Password too short!\n");
        return 0;
    }

    for(i = 0; password[i] != '\0'; i++) {
        if(isdigit(password[i]))
            hasDigit = 1;
    }

    if(hasDigit)
        printf("Password is valid\n");
    else
        printf("Password must contain at least one digit\n");

    return 0;
}
