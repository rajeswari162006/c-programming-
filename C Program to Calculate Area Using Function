#include <stdio.h>

// Function to calculate area
float area(float length, float width) {
    return length * width;
}

int main() {
    float length, width;

    printf("Enter length: ");
    scanf("%f", &length);

    printf("Enter width: ");
    scanf("%f", &width);

    printf("Area of rectangle: %.2f\n", area(length, width));

    return 0;
}
