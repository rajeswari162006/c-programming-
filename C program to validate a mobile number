#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char mobile[20];
    printf("Enter mobile number: ");
    scanf("%s", mobile);

    int len = strlen(mobile);
    int valid = 1;

    if(len != 10) {
        valid = 0;
    } else if(mobile[0] < '6' || mobile[0] > '9') {
        valid = 0;
    } else {
        for(int i = 0; i < 10; i++) {
            if(!isdigit(mobile[i])) {
                valid = 0;
                break;
            }
        }
    }

    if(valid)
        printf("Valid mobile number\n");
    else
        printf("Invalid mobile number\n");

    return 0;
}
